<?xml version="1.0"?>
<block>
  <name>Modified Miller Decoder</name>
  <key>nfc_modified_miller_decoder</key>
  <category>[NFC]</category>
  <import>import nfc</import>
  <make>nfc.modified_miller_decoder($sample_rate)</make>
  <param>
    <name>Sample Rate</name>
    <key>sample_rate</key>
    <type>real</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
